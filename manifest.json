{
  "name": "Form Autofiller",
  "version": "1.0",
  "description": "Autofills forms based on registered information",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus",
    "declarativeContent"
  ],
  "options_page": "layouts/options/options.html",
  "manifest_version": 2
}
